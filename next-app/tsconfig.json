{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": ["cypress"]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/[CompanyHeaderDetails].tsx", "pages/_app.tsx", "pages/companyDetails/index.tsx",".eslintrc.js", "cypress.config.js", "cypress/plugins/index.js"],
  "exclude": ["node_modules", "db-scripts.ts"]
}
